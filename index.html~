<head>    
  <title>scorm</title>    
  <script  src=https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js></script>  
</head>  
<body>    
  <iframe src=fsSCORM.html width='600px' height='600px'></iframe>    
  <script>      (function() {        var datamodel, save;        datamodel = {          'cmi.core.lesson_status': 'not attempted',          'cmi.learner_name': 'Tatum Bisley',          'cmi.learner_id': '4547',          'cmi.core.student_name': 'Tatum Bisley',          'cmi.core.student_id': '4547',          'cmi.launch_data': ''        };        save = function() {          return $.ajax({            type: 'put',            url: 'http://mobiledev.fuseuniversal.com/api/v1.5/contents/11533/scorm-data',            data: {              data: datamodel            }          });        };        window.API = {          LMSInitialize: function() {            return 'true';          },          LMSFinish: function() {            return 'true';          },          LMSGetValue: function(key) {            return datamodel[key] || '';          },          LMSSetValue: function(key, val) {            datamodel[key] = val;            save();            if (key === 'cmi.core.lesson_status' && val === 'completed') {              $('.scorm-course-completed').removeClass('hidden');            }            return 'true';          },          LMSCommit: function() {            return 'true';          },          LMSGetLastError: function() {            return '0';          },          LMSGetErrorString: function() {            return '';          },          LMSGetDiagnostic: function() {            return '';          }        };      }).call(this);    
  </script>  
</body>
</html>
